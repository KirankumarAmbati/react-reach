{
  "name": "react-reach",
  "version": "0.0.0-semantically-released",
  "description": "A small library for react to communicate with GraphQL",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build": "babel src/index.js -o dist/index.js",
    "commit": "git-cz",
    "test": "mocha test/temp.spec.js -w --compilers js:babel-core/register",
    "test:single": "mocha test/temp.spec.js --compilers js:babel-core/register",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kennetpostigo/react-reach.git"
  },
  "keywords": [
    "reach",
    "functional",
    "react"
  ],
  "author": "Kennet Postigo <kennetfpostigo@gmail.com> (http://kpostigo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kennetpostigo/react-reach/issues"
  },
  "homepage": "https://github.com/kennetpostigo/react-reach#readme",
  "devDependencies": {
    "babel-core": "6.1.21",
    "babel-istanbul": "0.5.8",
    "babel-preset-es2015": "6.1.18",
    "chai": "3.4.1",
    "codecov.io": "0.1.6",
    "commitizen": "2.4.4",
    "cz-conventional-changelog": "1.1.4",
    "ghooks": "1.0.0",
    "istanbul": "0.4.0",
    "mocha": "2.3.3",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single"
    }
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
